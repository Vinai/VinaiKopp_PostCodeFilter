{
  "name": "vinaikopp/postcodefilter",
  "description": "ShippingAddress PostCodeFilter Magento Extension",
  "authors": [
    {
      "name": "Vinai Kopp",
      "email": "vinai@netzarbeiter.com"
    }
  ],
  "require": {
    "php": "~5.5.0|~5.6.0",
    "magento/magento-composer-installer": ">=0.0.1,<100"
  },
  "type": "magento2-module",
  "license": [
    "BSD-3-Clause"
  ],
  "extra": {
    "map": [
      [
        "src/mage2/*",
        "VinaiKopp/PostCodeFilter"
      ]
    ]
  },
  "autoload": {
    "psr-4": {
      "VinaiKopp\\PostCodeFilter\\": [
        "src/lib/VinaiKopp/PostCodeFilter",
        "src/mage2/"
      ]
    }
  }
}
